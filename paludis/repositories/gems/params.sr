make_class_RepositoryParams()
{
    visible

    key environment "Environment *"
    key location FSEntry
    key install_dir FSEntry
    key sync std::string
    key sync_options std::string
    key builddir FSEntry

    allow_named_args
}

make_class_InstalledRepositoryParams()
{
    visible

    key environment "Environment *"
    key builddir FSEntry
    key install_dir FSEntry
    key root FSEntry

    allow_named_args
}

